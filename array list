import java.util.*;
public class Samplearray
{
    public static void main(String arr[])
    {
        try
        {
            List l=new ArrayList();
            l.add(1);
            l.add(2);
            l.add(3);
            l.remove(2);
            System.out.println(1);
        }
        catch(Exception e)
        {
            System.out.println(e);
        }
    }
}
